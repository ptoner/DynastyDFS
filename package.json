    "http-server": "^0.10.0",
    "large-web": "0.0.14",
    "moment": "2.24.0",
    "ts-node": "8.3.0",
    "tsify": "4.0.1",
    "typescript": "3.5.3",
    "ts-mocha": "6.0.0",
    "mocha": "6.2.0"
  },
  "dependencies": {
    "orbit-db": "0.22.1",
    "ipfs": "0.38.0",
    "ethers": "4.0.33",
